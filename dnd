
#import statement
from random import randrange

#input
difficulty = int(input("Enter the DC: "))

#processing and output
player_roll = randrange(1,21) # randrange never includes b; it never goes up to b

print(f"The player has rolled {player_roll} on their D20")

#Conditions if you win or not!!
if player_roll >= difficulty:
     print (f'The player was successful as {player_roll} >= {difficulty}')


else: 
     print(f'The player was unsuccessfull, as {player_roll} < {difficulty} ')







